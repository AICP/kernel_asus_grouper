TOOLCHAINDIR=~/toolchain

TOOLCHAIN=arm-eabi-4.6

CROSS_COMPILE=$TOOLCHAINDIR/$TOOLCHAIN/bin/arm-eabi-

LOCAL_BUILD_DIR=~/grouper/kernels

TARGET_DIR=~/grouper/kernels/updates

FTPTARGETDIR="/kernels/grouper"

SYSTEM_PARTITION="/dev/block/platform/sdhci-tegra.3/by-name/APP"

DEFCONFIG=grouper_funky_defconfig

FLASH_BOOT='/dev/block/mmcblk0p2'

PHONE='Asus Nexus7(Grouper)'

# ----

N_CORES=8
VERSION=Funky-Kernel-v1.1

# ----

BANNER=`cat <<EOF
ui_print("Funky Kernel");
ui_print("for");
ui_print("$PHONE");
ui_print("$VERSION");
EOF`
